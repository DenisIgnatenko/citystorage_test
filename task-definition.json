{
  "family": "citystorage-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::546259008126:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "citystorage-test",
      "image": "546259008126.dkr.ecr.us-east-1.amazonaws.com/citystorage-test",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/citystorage-task",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        { "name": "SPRING_PROFILES_ACTIVE", "value": "prod" },
        { "name": "DB_URL", "value": "jdbc:postgresql://citystorage-db.cyna00y6scf6.us-east-1.rds.amazonaws.com:5432/city_storage" },
        { "name": "DB_USERNAME", "value": "postgres" },
        { "name": "DB_PASSWORD", "value": "postgres" }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "4096"
}