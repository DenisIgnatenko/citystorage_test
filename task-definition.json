{
  "family": "citystorage-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::546259008126:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "citystorage-test",
      "image": "546259008126.dkr.ecr.us-east-1.amazonaws.com/citystorage-test",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/citystorage-task",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "4096"
}